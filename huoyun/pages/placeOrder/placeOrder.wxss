/* pages/placeOrder/placeOrder.wxss */
page {
  background: #F5F5F5;
  /* height: 100%; */
  overflow: scroll;
}

.container {
  /* min-height: 100%; */
  padding-top: 26rpx;
  /* padding-bottom: 120rpx; */
  /* overflow: hidden; */
}

.info-box-top {
  background: white;
  border-radius: 20rpx;
  margin: 0rpx 24rpx 0rpx;
  padding: 20rpx 25rpx 0;
  box-shadow: 0px 2rpx 10rpx 0rpx rgba(217, 215, 209, 0.5);
  z-index: 2;
  position: relative;
}

.info-box-top .ico {
  position: relative;
}

.info-box-top .ico image {
  width: 48rpx;
  height: 48rpx;
  object-fit: contain;
  position: relative;
}

.info-box-top .ico>.title {
  font-size: 30rpx;
  color: #333;
}

.info-box-top .area-box .start-area {
  position: relative;
}

.info-box-top .area-box .start-area::after {
  content: " ";
  position: absolute;
  height: 100%;
  width: 0px;
  border-left: 1px dashed #E6E6E6;
  bottom: 0;
  top: 58rpx;
  left: 24rpx;
}

.info-box-top .area-box .area-text {
  font-size: 30rpx;
  color: #333;
}

.info-box-top .tips {
  font-size: 30rpx;
  color: #ccc;
}

.info-box-top .guide {
  width: 28rpx;
  height: 28rpx;
  font-size: 0;
}

.info-box-top .guide image {
  width: 100%;
  height: 100%;
  object-fit: contain;
}

.info-box-top .guide>.title {
  font-size: 30rpx;
  color: #333;
}

.info-box-top .ico {
  width: 80rpx;
  text-align: left;
  font-size: 0;
  flex-shrink: 0;
}

.info-box-top .area-box .picker {
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
  z-index: 99;
}

.info-box-top .area-box .picker .right {
  width: 100%;
}

.info-box-top .area-box .picker .right .tips {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  border-bottom: 1px solid #E6E6E6;
  height: 80rpx;
  padding-left: 0rpx;
  color: #333333;
}

/* .info-box-top .area-box .picker .right .person-info{
  padding: 30rpx 0;
  display: flex;
} */
.info-box-top .area-box .end-area .picker .right {
  border-bottom: none;
}

.info-box-top .area-box .end-area {}

.info-box-top .area-box .person-info {
  padding: 30rpx 0 30rpx 80rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.info-box-top .area-box .person-info .person {
  width: 100%;
}

.info-box-top .area-box .person-info .person .title {
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
  margin-bottom: 10rpx;
}

.info-box-top .area-box .person-info .person .info {
  font-size: 28rpx;
  color: #999999;
}

.info-box-top .area-box .person-info .person .info .top {
  font-size: 30rpx;
  color: #666;
  margin-bottom: 5rpx;
}

.info-box-top .area-box .person-info .address {
  font-size: 28rpx;
  color: #666666;
  padding-left: 20rpx;
  border-left: 1px solid #E6E6E6;
  flex-shrink: 0;
}

.info-box-middle {
  background: white;
  border-radius: 0rpx;
  margin: 24rpx 24rpx 25rpx;
  padding: 0 25rpx;
  box-shadow: 0px 2rpx 10rpx 0rpx rgba(217, 215, 209, 0.5);
  z-index: 2;
  position: relative;
  border-radius: 20rpx;
}

.info-box-middle .ico {
  position: relative;
}

.info-box-middle .ico image {
  width: 48rpx;
  height: 48rpx;
  object-fit: contain;
  position: relative;
}

.info-box-middle .ico .title {
  font-size: 30rpx;
  color: #333;
}

.info-box-middle .area-box .start-area {
  position: relative;
}

.info-box-middle .area-box .start-area picker {
  position: relative;
  z-index: 9999;
}

.info-box-middle .area-box .area-text {
  font-size: 30rpx;
  color: #333;
}

.info-box-middle .tips {
  font-size: 30rpx;
  color: #ccc;
  width: 100%;
}

.info-box-middle .inp-box .tips {
  width: 100%;
}

.info-box-middle .guide {
  width: 28rpx;
  height: 28rpx;
  font-size: 30rpx;
  color: #333333;
  flex-shrink: 0;
}

.info-box-middle .guide image {
  width: 100%;
  height: 100%;
  object-fit: contain;
}

.info-box-middle .guide .title {
  font-size: 30rpx;
  color: #333;
}

.info-box-middle .ico {
  width: 120rpx;
  text-align: left;
  font-size: 0;
  flex-shrink: 0;
}

.info-box-middle .area-box .start-area {
  border-bottom: 1px solid #E6E6E6;
}

.info-box-middle .area-box .picker {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.info-box-middle .area-box .picker .right {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 116rpx;
  padding-left: 45rpx;
}

.info-box-middle .area-box .end-area .picker .right {
  border-bottom: none;
}

.info-box-middle .area-box .end-area {
  border-bottom: 1px solid #E6E6E6;
}

.info-box-middle .inp-box {
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #E6E6E6;
}

.info-box-middle .inp-box:last-child {
  border-bottom: none;
}

.info-box-middle .inp-box .right {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 116rpx;
  padding-left: 45rpx;
}

.info-box-middle .inp-box .right .guide {
  width: auto;
}

.info-box-middle .inp-box .right input {
  width: 100%;
  color: #333;
}

.info-box-middle .tit {
  border-bottom: 1px solid #E6E6E6;
  padding-top: 30rpx;
  padding-bottom: 20rpx;
  display: flex;
  align-items: flex-end;
}

.info-box-middle .tit .img-box {
  width: 48rpx;
  height: 48rpx;
  font-size: 0;
  margin-right: 10rpx;
}

.info-box-middle .tit .img-box image {
  width: 100%;
  height: 100%;
  object-fit: contain;
}

.info-box-middle .tit .text {
  font-size: 30rpx;
  color: #333;
  font-weight: bold;
  line-height: 1.4;
}

.way-box {
  background: white;
  border-radius: 40rpx;
  margin: 0 24rpx 24rpx;
  padding: 0 25rpx;
  box-shadow: 0px 2rpx 10rpx 0rpx rgba(217, 215, 209, 0.5);
}

.way-box .tit {
  border-bottom: 1px solid #E6E6E6;
  padding-top: 30rpx;
  padding-bottom: 20rpx;
  display: flex;
  align-items: flex-end;
}

.way-box .tit .img-box {
  width: 48rpx;
  height: 48rpx;
  font-size: 0;
  margin-right: 10rpx;
}

.way-box .tit .img-box image {
  width: 100%;
  height: 100%;
  object-fit: contain;
}

.way-box .tit .text {
  font-size: 30rpx;
  color: #333;
  font-weight: bold;
  line-height: 1.4;
}

.way-box .way label {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 116rpx;
  border-bottom: 1px solid #E6E6E6;
}

.way-box .way label .name {
  font-size: 30rpx;
  color: #333;
  display: inline-block;
}

.way-box .way label .tips {
  font-size: 26rpx;
  color: #666;
  display: inline-block;
}

.remark-box {
  padding: 30rpx;
  background: white;
  border-radius: 20rpx;
  box-sizing: border-box;
  margin: 0 24rpx 24rpx;
}

.remark-box .tit {
  display: flex;
  align-items: flex-end;
  justify-content: space-between;
  margin-bottom: 20rpx;
}

.remark-box .tit .left {
  font-size: 30rpx;
  display: flex;
}

.remark-box .tit .left .text {
  font-size: 30rpx;
  color: #333333;
}

.remark-box .tit .right {
  font-size: 30rpx;
  color: #B3B3B3;
}

.remark-box textarea {
  /* width: 100%; */
  padding: 20rpx;
  height: 100rpx;
}

.hetong-box {
  margin: 0 24rpx 24rpx;
  display: flex;
  align-items: center;
}

/*checkbox 整体大小  */
.hetong-box .checkbox {
  font-size: 26rpx;
  color: #333;
}

.hetong-box checkbox {
  width: 40rpx;
  height: 40rpx;
  border: 1px solid #CCCCCC;
  border-radius: 50%;
  margin-right: 10rpx;
}

/*checkbox 选项框大小  */
.hetong-box checkbox .wx-checkbox-input {
  width: 45rpx;
  height: 45rpx;
  border-radius: 50rpx;
  transform: translate(-5rpx, -5rpx);
}

/*checkbox选中后样式  */
.hetong-box checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  background: #FFC53B;
  border: 1px solid #FFC53B;
}

/*checkbox选中后图标样式  */
.hetong-box checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 28rpx;
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
  font-size: 22rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  /* -webkit-transform: translate(-50%, -50%) scale(1); */
}

.hetong-box .tongyi {
  color: #FFC53B;
  font-size: 26rpx;
}

.add-address {
  /* position: fixed; */
  width: 100%;
  /* left: 0;
  bottom: 0; */
  padding: 16rpx 24rpx;
  background: white;
  box-sizing: border-box;
  z-index: 3;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.add-address .text-box .top {
  display: flex;
  justify-content: flex-start;
  align-items: flex-end;
}

.add-address .text-box .top .title {
  font-size: 28rpx;
  color: #333333;
}

.add-address .text-box .top .price {
  font-size: 34rpx;
  color: #FF5E62;
  font-weight: bold;
}

.add-address .text-box .bottom {
  font-size: 26rpx;
  color: #999999;
}

.add-address .but-box {}

.add-address .but-box button {
  width: 340rpx;
  height: 88rpx;
  background: #FFC53B;
  border-radius: 44rpx;
  font-size: 34rpx;
  color: #333333;
  font-weight: 400;
  display: flex;
  align-items: center;
  justify-content: center;
}



.model {
  position: fixed;
  /* top: -100%; */
  left: 0;
  right: 0;
  height: 100%;
  margin: auto auto;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  align-items: center;
  transition: .5s;
  z-index: 3;
}

.model .content {
  padding-top: 40rpx;
  padding-bottom: 40rpx;
  position: relative;
  background: #FFFFFF;
  border-radius: 20rpx;
  margin: 0 45rpx 0;
  width: 100%;
  height: 80%;
  overflow: hidden;
}

.model .content .img-box {
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  right: 20rpx;
  top: 15rpx;
}

.model .content .img-box image {
  width: 100%;
  height: 100%;
}

.model .content .title {
  font-size: 32rpx;
  color: #333333;
  text-align: center;
  font-weight: bold;
}

.model .content .cont {
  margin: 40rpx 40rpx 0;
  height: 84%;
  overflow-y: scroll;
}

.model .content .cont .list {
  margin-bottom: 40rpx;
}

.model .content .cont .list .top {
  color: #333333;
  font-size: 28rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}

.model .content .cont .list .bottom {
  color: #333333;
  font-size: 28rpx;
}

.model .content .but-box {
  margin: 0;
  position: absolute;
  bottom: 0px;
  left: 50%;
  transform: translateX(-50%);
  width: 90%;
  box-sizing: border-box;
}

.model .content .but-box button {
  width: 100%;
  height: 88rpx;
  background: white;
  /* border-radius: 44rpx; */
  font-size: 34rpx;
  color: #FFC53B;
  border-top: 1px solid #E6E6E6;
  font-weight: 200;
}