page {
  background: #F5F5F5;
  height: 100%;
  /* padding: 0 24rpx; */
  box-sizing: border-box;
}
.title-box{
  margin: 0 24rpx;
  margin-top: 24rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  background: white;
  border-radius: 20rpx;
  padding: 35rpx 30rpx;
}
.title-box .img-box{
  width: 90rpx;
  height: 90rpx;
  font-size: 0;
  flex-shrink: 0;
  margin-right: 20rpx;
}
.title-box .img-box image{
  width: 100%;
  height: 100%;
}
.title-box .info .top{
  font-size: 36rpx;
  font-weight: bold;
  color: #333333;
  margin-bottom: 20rpx;
  line-height: 1;
}
.title-box .info .bottom{
  font-size: 26rpx;
  font-weight: 400;
  color: #999999;
  line-height: 1;
}



.mode-box{
  margin: 0 24rpx;
  margin-top: 24rpx;
  background: #FFFFFF;
  border-radius: 20rpx;
  padding: 0rpx 30rpx;
}
.mode-box .top{
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1px solid #E6E6E6;
}
.mode-box .bottom{
  display: flex;
  align-items: center;
  height: 90rpx;
}
.mode-box .bottom .one{
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
}
.mode-box .bottom .two{
  font-size: 26rpx;
  font-weight: 400;
  color: #999999;
}

.info-box-top{
  margin: 0 24rpx;
  background: white;
  border-radius: 20rpx;
  /* margin: 0rpx 24rpx 0rpx; */
  margin-top: 24rpx;
  padding: 0rpx 25rpx 0;
  /* box-shadow: 0px 2rpx 10rpx 0rpx rgba(217, 215, 209, 0.5); */
  z-index: 2;
  position: relative;
  overflow: hidden;
}
.info-box-top .tit{
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1px solid #E6E6E6;
}
.info-box-top .ico{
  position: relative;
}
.info-box-top .ico image{
  width: 48rpx;
  height: 48rpx;
  object-fit: contain;
  position: relative;
}
.info-box-top .ico > .title{
  font-size: 30rpx;
  color: #333;
}
.info-box-top .area-box{
  overflow: hidden;
  margin-top: 30rpx;
}
.info-box-top .area-box .start-area{
  position: relative;
}
.info-box-top .area-box .start-area {
  margin-bottom: 50rpx;
}
.info-box-top .area-box .start-area::after{
  content: " ";
  position: absolute;
  height: 100%;
  width: 0px;
  border-left: 1px dashed #E6E6E6;
  bottom: 0;
  top: 58rpx;
  left: 24rpx;
}
.info-box-top .area-box .area-text{
  font-size: 30rpx;
  color: #333;
}
.info-box-top .tips{
  font-size: 30rpx;
  color: #ccc;
}
.info-box-top .guide{
  width: 28rpx;
  height: 28rpx;
  font-size: 0;
}
.info-box-top .guide image{
  width: 100%;
  height: 100%;
  object-fit: contain;
}
.info-box-top .guide > .title{
  font-size: 30rpx;
  color: #333;
}
.info-box-top .ico{
  width: 80rpx;
  text-align: left;
  font-size: 0;
  flex-shrink: 0;
}
.info-box-top .area-box .picker{
  display: flex;
  align-items: flex-start;
  justify-content: space-between;
  position: relative;
  z-index: 99;
}
.info-box-top .area-box .picker .right{
  width: 100%;
}
.info-box-top .area-box .picker .right .tips{
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  border-bottom: 1px solid #E6E6E6;
  height: 80rpx;
  padding-left: 0rpx;
  color: #333333;
}
.info-box-top .area-box .end-area .picker .right{
  border-bottom: none;
}
.info-box-top .area-box .end-area{
  margin-bottom: 30rpx;
}
.info-box-top .area-box .person{
  width: 100%;
}
.info-box-top .area-box .person .title{
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
  margin-bottom: 10rpx;
}
.info-box-top .area-box .person .info{
  font-size: 28rpx;
  color: #999999;
}
.info-box-top .area-box .address{
  font-size: 28rpx;
  color: #666666;
  padding-left: 20rpx;
  border-left: 1px solid #E6E6E6;
  flex-shrink: 0;
}

.info-box{
  margin: 0 24rpx;
  background: white;
  border-radius: 20rpx;
  /* margin: 0rpx 24rpx 0rpx; */
  margin-top: 24rpx;
  padding: 0rpx 25rpx 0;
  /* box-shadow: 0px 2rpx 10rpx 0rpx rgba(217, 215, 209, 0.5); */
  z-index: 2;
  position: relative;
  overflow: hidden;
}
.info-box .tit{
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1px solid #E6E6E6;
}
.info-box .cont{
  padding: 40rpx 0;
}
.info-box .cont .lis {
  margin-bottom: 30rpx;
  display: flex;
  justify-content: space-between;
}
.info-box .cont .lis:last-child {
  margin-bottom: 0;
}
.info-box .cont .lis .name {
  font-size: 30rpx;
  font-weight: 400;
  color: #999999;
  display: inline-block;
  margin-right: 10rpx;
}
.info-box .cont .lis .tex {
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
  display: inline-block;
}

.price-box{
  background: white;
  border-radius: 20rpx;
  margin: 0rpx 24rpx 0rpx;
  margin-top: 24rpx;
  padding: 0rpx 25rpx 0;
  /* box-shadow: 0px 2rpx 10rpx 0rpx rgba(217, 215, 209, 0.5); */
  z-index: 2;
  position: relative;
  overflow: hidden;
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 120rpx;
}
.price-box .left{
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
}
.price-box .right{
  font-size: 40rpx;
  font-weight: 400;
  color: #FF5E62;
}


.add-address{
  /* position: fixed; */
  width: 100%;
  /* left: 0;
  bottom: 0; */
  padding: 16rpx 24rpx;
  background: white;
  box-sizing: border-box;
  z-index: 3;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 50rpx;
}
.add-address .text-box .top {
  display: flex;
  justify-content: flex-start;
  align-items: flex-end;
}
.add-address .text-box .top .title {
  font-size: 28rpx;
  color: #333333;
}
.add-address .text-box .top .price {
  font-size: 34rpx;
  color: #FF5E62;
  font-weight: bold;
}
.add-address .text-box .bottom {
  font-size: 26rpx;
  color: #999999;
}
.add-address .but-box{
  display: flex;
}
.add-address .but-box button{
  width: 150rpx;
  height: 68rpx;
  line-height: 68rpx;
  padding: 0;
  background: #FFC53B;
  border-radius: 44rpx;
  font-size: 26rpx;
  color: #333333;
  font-weight: 400;
  margin-left: 10rpx;
}

.add-address .but-box button.cacel{
  color: #666666;
  font-size: 26rpx;
  background: transparent;
  border: 1px solid #666;
}