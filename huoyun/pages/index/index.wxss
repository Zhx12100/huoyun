/**index.wxss**/
page{
  background: #F5F5F5;
}
.banner{
  height: 460rpx;
  width: 100%;
}
.banner swiper{
  height: 100%;
}
.banner .swiper-item{
  height: 100%;
}
.banner .swiper-item image{
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.banner image{
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.info-box{
  background: white;
  border-radius: 40rpx;
  margin: -58rpx 24rpx 25rpx;
  padding: 0 25rpx;
  box-shadow: 0px 2rpx 10rpx 0rpx rgba(217, 215, 209, 0.5);
  z-index: 2;
  position: relative;
}
.info-box .ico{
  position: relative;
}
.info-box .ico image{
  width: 48rpx;
  height: 48rpx;
  object-fit: contain;
  position: relative;
}
.info-box .ico .title{
  font-size: 30rpx;
  color: #333;
}
.info-box .area-box .start-area{
  position: relative;
}
.info-box .area-box .start-area picker{
  position: relative;
  z-index: 9999;
}
.info-box .area-box .start-area::after{
  content: " ";
  position: absolute;
  height: 100%;
  width: 0px;
  border-left: 1px dashed #E6E6E6;
  bottom: 0;
  top: 58rpx;
  left: 24rpx;
}
.info-box .area-box .area-text{
  font-size: 30rpx;
  color: #333;
}
.info-box .tips{
  font-size: 30rpx;
  color: #ccc;
}
.info-box .guide{
  width: 28rpx;
  height: 28rpx;
  font-size: 0;
}
.info-box .guide image{
  width: 100%;
  height: 100%;
  object-fit: contain;
}
.info-box .guide .title{
  font-size: 30rpx;
  color: #333;
}
.info-box .ico{
  width: 80rpx;
  text-align: left;
  font-size: 0;
  flex-shrink: 0;
}
.info-box .area-box .picker{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.info-box .area-box .picker .right{
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  border-bottom: 1px solid #E6E6E6;
  height: 116rpx;
  padding-left: 45rpx;
}
.info-box .area-box .end-area .picker .right{
  border-bottom: none;
}
.info-box .area-box .end-area{
  border-bottom: 1px solid #E6E6E6;
}
.info-box .inp-box{
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #E6E6E6;
}
.info-box .inp-box:last-child{
    border-bottom: none;
}
.info-box .inp-box .right{
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 116rpx;
  padding-left: 45rpx;
}
.info-box .inp-box .right .guide{
  width: auto;
}
.info-box .inp-box .right input{
  width: 100%;
  color: #333;
}
.way-box{
  background: white;
  border-radius: 40rpx;
  margin: 0 24rpx 50rpx;
  padding: 0 25rpx;
  box-shadow: 0px 2rpx 10rpx 0rpx rgba(217, 215, 209, 0.5);
}
.way-box .title{
  font-size: 30rpx;
  color: #333;
  font-weight: bold;
  border-bottom: 1px solid #E6E6E6;
  padding-top: 40rpx;
  padding-bottom: 20rpx;
  line-height: 1;
}
.way-box .way label{
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 116rpx;
  border-bottom: 1px solid #E6E6E6;
}
.way-box .way label .name{
  font-size: 30rpx;
  color: #333;
  display: inline-block;
}
.way-box .way label .tips{
  font-size: 26rpx;
  color: #666;
  display: inline-block;
}
.button-box{
  margin: 0 24rpx 50rpx;
}
.button-box .submit-but{
  width: 100%!important;
  height: 88rpx;
  background: #FFC53B;
  border-radius: 50px;
  font-size: 34rpx;
  color: #333;
}