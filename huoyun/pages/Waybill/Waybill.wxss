page {
  background: #F5F5F5;
  height: 100%;
  overflow: hidden;
}

.container {
  background: white;
}

.inp-box {
  border-top: 1px solid transparent;
  border-bottom: 1px solid transparent;
  background: white;
}

.input-box {
  background: #F5F5F5;
  border-radius: 34rpx;
  padding: 0 34rpx;
  margin: 30rpx 24rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  overflow: hidden;
}

.input-box .img-box {
  width: 30rpx;
  height: 30rpx;
  font-size: 0;
  flex-shrink: 0;
  margin-right: 10rpx;
}

.input-box .img-box image {
  width: 100%;
  height: 100%;
  object-fit: fill;
}

.input-box input {
  width: 100%;
  height: 68rpx;
  font-size: 28rpx;
  color: #333;
}


.swiper-tab {
  width: 100%;
  text-align: center;
  line-height: 80rpx;
  background: white;
}

.swiper-tab-list {
  font-size: 30rpx;
  display: inline-block;
  width: 20%;
  color: #777777;
}

.on {
  color: #333;
  font-weight: bold;
  border-bottom: 4rpx solid #FFC53B;
}

.swiper-box {
  display: block;
  /* height: 100%; */
  width: 100%;
  overflow: hidden;
}

.swiper-box .list {
  margin: 24rpx 24rpx 0;
  background: white;
  border-radius: 14rpx;
  padding: 0 20rpx 24rpx;
}
.swiper-box .list .tit {
  font-size: 28rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #EDEDED;
}
.swiper-box .list .tit .no .select{
  width: 40rpx;
  height: 40rpx;
  box-sizing: border-box;
  border-radius: 50%;
  border: 1px solid #CCCCCC;
  margin-right: 20rpx;
}
.swiper-box .list .tit .no .select_active{
  width: 40rpx;
  height: 40rpx;
  font-size: 0;
  margin-right: 20rpx;
}
.swiper-box .list .tit .no .select_active image{
  width: 100%;
  height: 100%;
}
.swiper-box .list .tit .no{
  display: flex;
  align-items: center;
}
.swiper-box .list .tit .no .text{
  color: #666666;
}
.swiper-box .list .tit .status {
  color: #333333;
}
.swiper-box .list .address {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 30rpx 0;
}
.swiper-box .list .address .area {
  text-align: center;
}
.swiper-box .list .address .area .dizhi {
  font-size: 34rpx;
  font-family: NotoSansHans;
  font-weight: bold;
  color: #333333;
  margin-bottom: 15rpx;
}
.swiper-box .list .address .area .name {
  font-size: 28rpx;
  font-family: NotoSansHans;
  font-weight: 400;
  color: #999999;
}
.swiper-box .list .address .jiant {
  width: 100rpx;
  font-size: 0;
}
.swiper-box .list .address .jiant image{
  width: 100%;
  height: 26rpx;
}
.swiper-box .list .info{
  font-size: 26rpx;
  color: #999;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  margin-bottom: 30rpx;
}
.swiper-box .list .info .lis{
  /* width: 25%; */
  margin-right: 50rpx;
  display: flex;
  align-items: center;
}
.swiper-box .list .info .lis .img-box{
  width: 36rpx;
  height: 36rpx;
  font-size: 0;
  margin-right: 6rpx;
}
.swiper-box .list .info .lis .img-box image{
  width: 100%;
  height: 100%;
}
.swiper-box .list .time{
  display: flex;
  align-items: center;
  font-size: 26rpx;
  color: #999;
  margin-bottom: 40rpx;
}
.swiper-box .list .time .img-box{
  width: 36rpx;
  height: 36rpx;
  font-size: 0;
  margin-right: 6rpx;
}
.swiper-box .list .time .img-box image{
  width: 100%;
  height: 100%;
}
.swiper-box .list .but-box {
  text-align: right;
}
.swiper-box .list .but-box button{
  width: 160rpx;
  height: 60rpx;
  border-radius: 50rpx;
  font-size: 28rpx;
  padding: 0;
  line-height: 60rpx;
  font-weight: 400;
  display: inline-block;
  margin-left: 20rpx;
}
.swiper-box .list .but-box button.cacel{
  border: 2rpx solid #CCCCCC;
  background: white;
  color: #ccc;
}
.swiper-box .list .but-box button.confirm{
  background: #FFC53B;
  color: #333;
}
.swiper-box .kaipiao-box{
  padding-bottom: 0rpx;
}
.caozuo{
  display: flex;
  justify-content: space-between;
  margin-top: 24rpx;
  align-items: center;
  padding-bottom: 24rpx;
}
.caozuo .piliang{
  background: #FFFFFF;
  border-radius: 0px 30px 30px 0px;
  width: 200rpx;
  height: 60rpx;
  /* line-height: 60rpx; */
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
}
.kaipiao-box .list:first-child{
  margin-top: 0;
}
.caozuo .piliang .img-box{
  width: 34rpx;
  height: 34rpx;
  font-size: 0;
  margin-right: 10rpx;
}
.caozuo .piliang .img-box image{
  width: 100%;
  height: 100%;
}

.caozuo .lishi{
  height: 60rpx;
  /* line-height: 60rpx; */
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #999;
}
.caozuo .lishi .img-box{
  width: 24rpx;
  height: 24rpx;
  font-size: 0;
  margin-right: 10rpx;
}
.caozuo .lishi .img-box image{
  width: 100%;
  height: 100%;
}
.swiper-box .kaipiao-box .list .time{
  margin-bottom: 20rpx;
}

.add-address{
  position: fixed;
  width: 100%;
  left: 0;
  bottom: 0;
  padding: 16rpx 24rpx;
  background: white;
  box-sizing: border-box;
  z-index: 3;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.add-address .text-box {
  display: flex;
  justify-content: flex-start;
  align-items: flex-end;
}
.add-address .text-box .bottom {
  font-size: 26rpx;
  color: #999999;
}
.add-address .but-box{

}
.add-address .but-box button{
  width: 340rpx;
  height: 88rpx;
  background: #FFC53B;
  border-radius: 44rpx;
  font-size: 34rpx;
  color: #333333;
  font-weight: 400;
}

.hetong-box{
  display: flex;
  align-items: center;
  margin-right: 60rpx;
}
/*checkbox 整体大小  */
.hetong-box .checkbox{
  font-size: 26rpx;
  color: #333;
}
.hetong-box checkbox {
  width: 40rpx;
  height: 40rpx;
  border: 1px solid #CCCCCC;
  border-radius: 50%;
  margin-right: 10rpx;
}
/*checkbox 选项框大小  */
.hetong-box checkbox .wx-checkbox-input {
  width: 45rpx;
  height: 45rpx;
  border-radius: 50rpx;
  transform: translate(-5rpx,-5rpx);
}
/*checkbox选中后样式  */
.hetong-box checkbox .wx-checkbox-input.wx-checkbox-input-checked {
   background: #FFC53B;
   border: 1px solid #FFC53B;
}
/*checkbox选中后图标样式  */
.hetong-box checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 28rpx;
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
  font-size: 22rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  /* -webkit-transform: translate(-50%, -50%) scale(1); */
}
.hetong-box .tongyi{
  color: #FFC53B;
  font-size: 26rpx;
}
.add-address .cacel{
  color: #666666;
  font-size: 28rpx;
}

/*  加载更多   */
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore .image{
  width:200rpx;
  height: 200rpx!important;
  font-size: 0;
  margin-bottom: 20rpx;
}
.weui-loadmore__tips {
  display: block;
  vertical-align: middle;
  text-align: center;
}