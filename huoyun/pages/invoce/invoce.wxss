page {
  background: #F5F5F5;
  height: 100%;
}

.list {
  margin: 24rpx 24rpx 0;
  background: white;
  border-radius: 14rpx;
  padding: 0 20rpx 24rpx;
}
.list .tit {
  font-size: 28rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #EDEDED;
}
.list .tit .no {
  color: #666666;
}
.list .tit .status {
  color: #333333;
}
.list .address {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 30rpx 0;
}
.list .address .area {
  text-align: center;
}
.list .address .area .dizhi {
  font-size: 34rpx;
  font-family: NotoSansHans;
  font-weight: bold;
  color: #333333;
  margin-bottom: 15rpx;
}
.list .address .area .name {
  font-size: 28rpx;
  font-family: NotoSansHans;
  font-weight: 400;
  color: #999999;
}
.list .address .jiant {
  width: 100rpx;
  font-size: 0;
}
.list .address .jiant image{
  width: 100%;
  height: 26rpx;
}
.list .info{
  font-size: 26rpx;
  color: #999;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  margin-bottom: 30rpx;
}
.list .info .lis{
  /* width: 25%; */
  margin-right: 50rpx;
  display: flex;
  align-items: center;
}
.list .info .lis .img-box{
  width: 36rpx;
  height: 36rpx;
  font-size: 0;
  margin-right: 6rpx;
}
.list .info .lis .img-box image{
  width: 100%;
  height: 100%;
}
.list .time{
  display: flex;
  align-items: center;
  font-size: 26rpx;
  color: #999;
  margin-bottom: 30rpx;
}
.list .time .img-box{
  width: 36rpx;
  height: 36rpx;
  font-size: 0;
  margin-right: 6rpx;
}
.list .time .img-box image{
  width: 100%;
  height: 100%;
}
.list .but-box {
  text-align: right;
}
.list .but-box button{
  width: 160rpx;
  height: 60rpx;
  border-radius: 50rpx;
  font-size: 28rpx;
  padding: 0;
  line-height: 60rpx;
  font-weight: 400;
  display: inline-block;
  margin-left: 20rpx;
}
.list .but-box button.cacel{
  border: 2rpx solid #CCCCCC;
  background: white;
  color: #ccc;
}
.list .but-box button.confirm{
  background: #FFC53B;
  color: #333;
}

/*  加载更多   */
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}